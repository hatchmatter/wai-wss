{
  "name": "wai-wss",
  "version": "1.1.0",
  "description": "Websocket server for WAI",
  "type": "module",
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx watch --env-file=.env.development src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "rm -fr dist",
    "build": "tsc && tsc-alias",
    "postbuild": "cp src/lib/agent/system.txt dist/lib/agent/system.txt"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.2.21",
    "@langchain/langgraph": "^0.1.1",
    "@langchain/redis": "^0.0.5",
    "@supabase/supabase-js": "^2.40.0",
    "bullmq": "^5.11.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "langchain": "^0.2.11",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.17.0"
  }
}
